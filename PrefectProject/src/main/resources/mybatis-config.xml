<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<!-- mybatis 설정 -->  
<configuration>
    <!-- mapUnderscoreToCamelCase 맵핑 -->
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
        <setting name="logImpl" value="LOG4J2"/>
        <setting name="jdbcTypeForNull" value="NULL"/>
    </settings>
    <typeAliases>
        <typeAlias type="com.pcwk.ehr.reply.domain.ReplyVO"   alias="ReplyVO"/>
        <typeAlias type="com.pcwk.ehr.code.domain.CodeVO"   alias="CodeVO"/>
        <typeAlias type="com.pcwk.ehr.file.domain.FileVO"   alias="FileVO"/>
        <typeAlias type="com.pcwk.ehr.user.domain.UserVO"   alias="UserVO"/>
        <typeAlias type="com.pcwk.ehr.board.domain.BoardVO" alias="BoardVO"/>
        <typeAlias type="com.pcwk.ehr.chart.domain.ScoreVO" alias="ScoreVO"/>
        <typeAlias type="com.pcwk.ehr.dm.domain.DmVO" alias="DmVO"/>
        <typeAlias type="com.pcwk.ehr.subject.domain.SubjectVO" alias="SubjectVO"/>
        <typeAlias type="com.pcwk.ehr.schedule.domain.ScheduleVO" alias="ScheduleVO"/>
        <typeAlias type="com.pcwk.ehr.calendar.domain.CalendarVO" alias="CalendarVO"/>
        <typeAlias type="com.pcwk.ehr.calendar.domain.WeekVO" alias="WeekVO"/>
        <typeAlias type="com.pcwk.ehr.licenses.domain.LicensesVO" alias="LicensesVO"/>
        <typeAlias type="com.pcwk.ehr.course.domain.CourseVO" alias="CourseVO"/>
        <typeAlias type="com.pcwk.ehr.attendance.domain.AttendanceVO" alias="AttendanceVO"/>
    </typeAliases>
</configuration>